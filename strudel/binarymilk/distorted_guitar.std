kick: s("bd bd:4 bd bd:4").bank("tr707").gain(slider(4, 0, 4, .1)).echo(2,1,1)
  ._punchcard().color("red")
snare: s("- sd - sd").bank("tr707")
clap: s("- - - - - - - <cp -> ").bank("tr707")
bassline: n("11 11 11 <11 21> 33 33 33 <11 [41 42 43 44 47]>")
  .sound("saw")
  .gain(slider(1.4,0,4,0.2))
  .lpf(slider(380, 100, 1000, 10))
  .hpf(slider(1000, 100, 1000, 10))
  .color("cyan")._scope()
bass_floor:n("11 12 11 <11 21>")
  // .scale("<C1:minor>")
  .someCyclesBy(.4,x=>x.scale("C1:minor"))
  .off(2/16, x=>x.ply("<2 1>").delay(.2))
  .sound("gm_distortion_guitar").gain(2)
  .color("cyan")._scope()

number_lady: n("<1 1 0 0 >")
  .sound("numbers")
  .fast(2)
  .delay(.5)
  .gain(.4)
  .jux(rev)


clap: n("- - - - - - - <cp -> ").scale("tr707")

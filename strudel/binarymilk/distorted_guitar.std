tingle: n("- 1 - 2 - 4 - <1 -> ").sound("pulse").scale("c4:minor")
  .almostNever(x=>x.adsr("0:0:.2:.3").gain(3.5).delay(.5))
  .room(1)

tangle: n("- - < - [3 5 3]> - - - - <1 ->").sound("pulse").scale("c2:minor")

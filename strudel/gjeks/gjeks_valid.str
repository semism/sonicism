setCps(140/60/4)

kick: s("sbd!4").distort("1:.7").orbit(2)
  ._scope()
snare: s("- sd - <sd:1 sd:2>").clip(".1")
hihat: s("white!16")
  .clip(".4 .2 .1").pan(".77 -.77 .44")

bike: n("<1 ->").scale("A1:minor").s("pulse, gm_trumpet")
  .room(2).roomsize(3)
  .detune(1).rel(10).beat(4, 64).gain(.3)

bass_lead: n(irand(8).seg(16).sometimesBy(.12, x=>x.add(7)))
  .scale("A2:minor").s("sawtooth, saw")
  // .rib(5, 2)
  .rib(5, "<1 1 1 2>")
// .rib("<0!4 5!4>", "<1 1 1 2>")
  .lpf(800).lpenv(2)
//   .lpq(20)

bass: n(irand(8).seg(8)) //start with 8th and play 16th notes to harmonize with lead
  .scale("A1:minor").s("sawtooth, saw")
  // .rib(5, 2)
  .rib(5, "<1 1 1 2>")
// .rib("<0!4 5!4>", "<1 1 1 2>")
  .lpf(2000).lpenv(2).lpq(0)


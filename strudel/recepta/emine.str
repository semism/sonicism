dahiretrofaza: note(notes.add("7"))
  .room(2)
  .s("gm_sitar, saw, pulse").detune(".5").gain(.5)
  .lpf(3000)
  .clip("<.1 .4 .5 .8>*16")
  .jux(rev)
  .delay(1)

bass_bounce: note(notes.add("-14, -7"))
  // .rib(4, 1/4)
  .s("saw").delay(.5)
  .decay(.4)
  .clip(rand.rib(43, 4))
  .lpf(200).lpenv(2).lpq(10)

bass_floor: note(notes.seg(16).add(-35))
  .rib(4, 1/16)
  .rel(.6)
  .s("saw").fm(2).fmh(2).lpf(2000).room(1)
  .gain(.3)

